<link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">
<script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>

<div class="container" style="overflow: auto; margin-bottom:150px; margin-top: 150px;" data-aos="fade-right" data-aos-duration="1000">
  <div class="card" style="border: 2px solid black; background-color: rgba(255, 255, 255, 0.8); color: black;">
    <% for (var page of request.WEB_CONFIG.PAGES.filter(p => p.type.toLowerCase() === type.toLowerCase())) { %>
      <%- include(`../pages/${page.name}`, { request }); %>
    <% } %>
  </div>
</div>

<script>
  AOS.init();
</script>
